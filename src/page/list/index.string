{{#list}}
<li class="p-item">
	<div class="p-img-con">
		<a href="./detail.html?productId={{id}}" class="link" target="_balnk">
			<img class ="p-img" src="{{imageHost}}{{mainImage}}" alt="{{name}}"  />
		</a>
	</div>
	<div class="p-price-con">
		<span class="p-price">¥{{price}}</span>
	</div>
	<div class="p-name-con">
		<a href="./detail.html?productId={{id}}" class="p-name" target="_blank">
			{{name}}
		</a>
	</div>
</li>
{{/list}}

{{^list}}
<p class="err-tips">很抱歉，找不到您要的商品!</p>
{{/list}}